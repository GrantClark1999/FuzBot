  "repository": {
    "type": "git",
    "url": "git+https://github.com/GrantClark209/fuzbo-bot.git"
  },
  "bugs": {
    "url": "https://github.com/GrantClark209/fuzbo-bot/issues"
  },
  "prettier": {
    "overrides": [
      {
        "files": [
          ".prettierrc",
          ".babelrc",
          ".eslintrc"
        ],
        "options": {
          "parser": "json"
        }
      }
    ],
    "singleQuote": true
  }
}
